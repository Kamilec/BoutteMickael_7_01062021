   ref=\"image\"\r\n            v-on:change=\"upload()\"\r\n          />\r\n        </p>\r\n      </div>\r\n      <br />\r\n      <button type=\"submit\" class=\"card-icon\" @click.prevent=\"sendPost()\">\r\n        <i class=\"fas fa-plus\"></i>\r\n      </button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import axios from 'axios';\r\n  export default {\r\n    name: 'Publish',\r\n\r\n    data() {\r\n      return {\r\n        userId: localStorage.getItem('userId'),\r\n        title: '',\r\n        content: '',\r\n        image: '',\r\n        likes: '',\r\n        token: localStorage.getItem('token'),\r\n      };\r\n    },\r\n\r\n    methods: {\r\n      upload() {\r\n        this.image = this.$refs.image.files[0];\r\n      },\r\n      sendPost() {\r\n        const formData = new FormData();\r\n        formData.append('userId', parseInt(localStorage.getItem('userId')));\r\n        formData.append('image', this.image, this.image.filename);\r\n        formData.append('title', this.title);\r\n        formData.append('content', this.content);\r\n        formData.append('likes', this.likes);\r\n        axios\r\n          .post('http://localhost:3000/post/create', formData, {\r\n            headers: { Authorization: 'Bearer ' + localStorage.token },\r\n          })\r\n          .then(() => this.$router.push({ path: 'posted' }));\r\n      },\r\n    },\r\n  };\r\n</script>\r\n\r\n<style scoped>\r\n  .card-icon {\r\n    display: inline-block;\r\n    outline: none;\r\n    cursor: pointer;\r\n    text-align: center;\r\n    transition: all 0.2s linear;\r\n    width: 30px;\r\n    margin-bottom: 20px;\r\n  }\r\n</style>\r\n"],"sourceRoot":""}]}