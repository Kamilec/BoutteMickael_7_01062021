,IAAA,GAAG,EAAC,OAEJ;AADK,IAAA,QAAM,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,MAAA,EAAF;AAAA,KAAA;AACX,GAPF,E,IAAA,E;;AAAA,GAQE,CAZJ,CAaI,CAdN,C,EAeA,U,EACA,YAAA,CAES,QAFT,EAES;AAFD,IAAA,KAAK,EAAC,yBAEL;AAF+B,IAAA,IAAI,EAAC,QAEpC;AAF6C,IAAA,KAAK,EAAC,WAEnD;AAFgE,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA,UAAA,MAAA;AAAA,aAAU,QAAA,CAAA,QAAA,EAAV;AAAA,KAAA,EAAkB,CAAA,SAAA,CAAlB,CAAA;AAErE,GAFT,E,CACE,U,CADF,C,CAjDF,E;;AAAA,GAqDI,CAvDN,C","sourcesContent":["<template>\r\n  <div class=\"card\">\r\n    <div class=\"card card-info\"> <h1>Post</h1></div>\r\n    <form\r\n      id=\"survey\"\r\n      enctype=\"multipart/form-data\"\r\n      @submit.prevent=\"sendPost()\"\r\n    >\r\n      <div>\r\n        <p>\r\n          <label for=\"title\" class=\"form-label\"><b>Titre: </b></label>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form__input\"\r\n            name=\"title\"\r\n            placeholder=\"Légendez-moi ...\"\r\n            v-model=\"title\"\r\n            id=\"title\"\r\n          />\r\n        </p>\r\n      </div>\r\n      <br />\r\n      <div>\r\n        <p>\r\n          <label for=\"content\" class=\"form-label\"> <b>Description:</b></label>\r\n          <textarea\r\n            name=\"content\"\r\n            id=\"content\"\r\n            cols=\"25\"\r\n            rows=\"2\"\r\n            v-model=\"content\"\r\n            placeholder=\"Déscription du post ...\"\r\n          ></textarea>\r\n        </p>\r\n      </div>\r\n      <br />\r\n      <div>\r\n        <p>\r\n          <label for=\"picture\" class=\"form-label\">\r\n            <b>Ajoutez une image: </b></label\r\n          >\r\n          <input\r\n            type=\"file\"\r\n            class=\"form__input\"\r\n            id=\"image\"\r\n            name=\"image\"\r\n            ref=\"image\"\r\n            v-on:change=\"upload()\"\r\n          />\r\n        </p>\r\n      </div>\r\n      <br />\r\n      <button title=\"Ajouter un nouveau post\" type=\"submit\" class=\"card-icon\" @click.prevent=\"sendPost()\">\r\n        <i class=\"fas fa-plus\"></i>\r\n      </button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import axios from 'axios';\r\n  export default {\r\n    name: 'Post',\r\n\r\n    data() {\r\n      return {\r\n        userId: localStorage.getItem('userId'),\r\n        title: '',\r\n        content: '',\r\n        image: '',\r\n        likes: '',\r\n        token: localStorage.getItem('token'),\r\n      };\r\n    },\r\n\r\n    methods: {\r\n      upload() {\r\n        this.image = this.$refs.image.files[0];\r\n      },\r\n      sendPost() {\r\n        const formData = new FormData();\r\n        formData.append('userId', parseInt(localStorage.getItem('userId')));\r\n        formData.append('image', this.image, this.image.filename);\r\n        formData.append('title', this.title);\r\n        formData.append('content', this.content);\r\n        formData.append('likes', this.likes);\r\n        axios\r\n          .post('http://localhost:3000/post/create', formData, {\r\n            headers: { Authorization: 'Bearer ' + localStorage.token },\r\n          })\r\n          .then(() => this.$router.push({ path: 'posted' }));\r\n      },\r\n    },\r\n  };\r\n</script>\r\n\r\n<style scoped>\r\n  .card-icon {\r\n    display: inline-block;\r\n    outline: none;\r\n    cursor: pointer;\r\n    text-align: center;\r\n    transition: all 0.2s linear;\r\n    width: 30px;\r\n    margin-bottom: 20px;\r\n  }\r\n</style>\r\n"],"sourceRoot":""}]}